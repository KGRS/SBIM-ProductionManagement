	<queryString>
		<![CDATA[SELECT
     JobFixed."SUB_DEPARTMENT_CODE" AS JobFixed_SUB_DEPARTMENT_CODE,
     ItemsAtDepartments."ItemCode" AS ItemsAtDepartments_ItemCode,
     ItemsAtDepartments."DepartmentCode" AS ItemsAtDepartments_DepartmentCode
