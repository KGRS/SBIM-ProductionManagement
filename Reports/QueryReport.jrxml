	<queryString>
		<![CDATA[SELECT
     JobRunning."JOB_ID" AS JobRunning_JOB_ID,
     JobRunning."PRODUCT_LEVEL_ITEM_CODE" AS JobRunning_PRODUCT_LEVEL_ITEM_CODE,
     JobRunning."ITEM_COUNT" AS JobRunning_ITEM_COUNT,
     ProductLevel1RawItems."PL1_ITEM_QUANTITY" AS ProductLevel1RawItems_PL1_ITEM_QUANTITY,
     ProductLevel1RawItems."ItemCode" AS ProductLevel1RawItems_ItemCode,
     ProductLevel1RawItems."QUANTITY" AS ProductLevel1RawItems_QUANTITY,
     Items."ItemName" AS Items_ItemName,
     Items."UnitPurchase" AS Items_UnitPurchase,
     Items."SupplierCode" AS Items_SupplierCode,
     Items."PurchasePrice" AS Items_PurchasePrice,
     Items."Quantity" AS Items_Quantity
FROM
     "dbo"."ProductLevel1RawItems" ProductLevel1RawItems INNER JOIN "dbo"."JobRunning" JobRunning ON ProductLevel1RawItems."PL1_ITEM_CODE" = JobRunning."PRODUCT_LEVEL_ITEM_CODE"
     INNER JOIN "dbo"."Items" Items ON ProductLevel1RawItems."ItemCode" = Items."ItemCode"
WHERE
     JobRunning."JOB_ID" = '']]>
