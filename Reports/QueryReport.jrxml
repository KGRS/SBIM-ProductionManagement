	<queryString>
		<![CDATA[SELECT
     ProductLevel1."PL1_ITEM_CODE" AS ProductLevel1_PL1_ITEM_CODE,
     ProductLevel1."PL1_ITEM_NAME" AS ProductLevel1_PL1_ITEM_NAME,
     ProductLevel1."PL1_ITEM_PRINT_NAME" AS ProductLevel1_PL1_ITEM_PRINT_NAME,
     ProductLevel1."UnitCode" AS ProductLevel1_UnitCode,
     ProductLevel1."SUB_DEPARTMENT_CODE" AS ProductLevel1_SUB_DEPARTMENT_CODE,
     ProductLevel1."VISIBILITY" AS ProductLevel1_VISIBILITY,
     ProductLevel1RawItems."PL1_ITEM_CODE" AS ProductLevel1RawItems_PL1_ITEM_CODE,
     ProductLevel1RawItems."PL1_ITEM_QUANTITY" AS ProductLevel1RawItems_PL1_ITEM_QUANTITY,
     ProductLevel1RawItems."ItemCode" AS ProductLevel1RawItems_ItemCode,
     ProductLevel1RawItems."QUANTITY" AS ProductLevel1RawItems_QUANTITY,
     SIUnits."UnitName" AS SIUnits_UnitName,
     SIUnits."PurchaseMeasurement" AS SIUnits_PurchaseMeasurement,
     SIUnits."IssueMeasurement" AS SIUnits_IssueMeasurement,
     SIUnits."BaseUnitCode" AS SIUnits_BaseUnitCode,
     SubDepartments."SUB_DEPARTMENT_NAME" AS SubDepartments_SUB_DEPARTMENT_NAME,
     SubDepartments."DepartmentCode" AS SubDepartments_DepartmentCode,
     Items."ItemName" AS Items_ItemName,
     Items."UnitPurchase" AS Items_UnitPurchase,
     Process."PROCESS_NAME" AS Process_PROCESS_NAME,
     ProductLevel1."PROCESS_CODE" AS ProductLevel1_PROCESS_CODE
FROM
     "dbo"."ProductLevel1RawItems" ProductLevel1RawItems INNER JOIN "dbo"."ProductLevel1" ProductLevel1 ON ProductLevel1RawItems."PL1_ITEM_CODE" = ProductLevel1."PL1_ITEM_CODE"
     INNER JOIN "dbo"."SIUnits" SIUnits ON ProductLevel1."UnitCode" = SIUnits."UnitCode"
     INNER JOIN "dbo"."SubDepartments" SubDepartments ON ProductLevel1."SUB_DEPARTMENT_CODE" = SubDepartments."SUB_DEPARTMENT_CODE"
     INNER JOIN "dbo"."Process" Process ON ProductLevel1."PROCESS_CODE" = Process."PROCESS_CODE"
     INNER JOIN "dbo"."Items" Items ON ProductLevel1RawItems."ItemCode" = Items."ItemCode"
WHERE
     ProductLevel1."PL1_ITEM_CODE" = 'PL1C2']]>
	</queryString>
