	<queryString>
		<![CDATA[SELECT
     JobFixed."JOB_FIXED_ID" AS JobFixed_JOB_FIXED_ID,
     JobFixed."JOB_FIXED_NAME" AS JobFixed_JOB_FIXED_NAME,
     JobFixed."PRODUCT_LEVEL" AS JobFixed_PRODUCT_LEVEL,
