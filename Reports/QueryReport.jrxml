     "dbo"."ProductLevel1" ProductLevel1 INNER JOIN "dbo"."JobFixed" JobFixed ON ProductLevel1."PL1_ITEM_CODE" = JobFixed."PRODUCT_LEVEL_ITEM_CODE"
