<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="QueryReport" language="groovy" pageWidth="595" pageHeight="842" columnWidth="555" leftMargin="20" rightMargin="20" topMargin="20" bottomMargin="20" uuid="e1d4b839-af8f-4a37-a540-31aeea9ca260">
	<property name="ireport.zoom" value="1.0"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<queryString>
		<![CDATA[SELECT
     SUM(JobFinished."ITEM_COUNT") AS JobFinished_ITEM_COUNT_SUM,
     SUM(JobFinished."ITEM_COUNT_COMPLETED") AS JobFinished_ITEM_COUNT_COMPLETED_SUM,
     COUNT(JobFinished."ITEM_COUNT") AS JobFinished_ITEM_COUNT_COUNT,
     COUNT(JobFinished."ITEM_COUNT_COMPLETED") AS JobFinished_ITEM_COUNT_COMPLETED_COUNT,
     JobFixed."PRODUCT_LEVEL_ITEM_CODE" AS JobFixed_PRODUCT_LEVEL_ITEM_CODE
FROM
     "dbo"."JobFinished" JobFinished INNER JOIN "dbo"."PLItemDifference" PLItemDifference ON JobFinished."JOB_ID" = PLItemDifference."JOB_ID"
     INNER JOIN "dbo"."JobFixed" JobFixed ON JobFinished."FIXED_JOB_ID" = JobFixed."JOB_FIXED_ID"
WHERE
     JobFixed."PRODUCT_LEVEL_ITEM_CODE" = '"+PRODUCT_LEVEL_ITEM_CODE+"'
 AND PLItemDifference."IS_WASTAGE" = 'No'
 AND JobFinished."IS_COMPLETE_CANCLE" = 'Completed'
GROUP BY
     JobFixed."PRODUCT_LEVEL_ITEM_CODE",
     JobFinished."ITEM_COUNT",
     JobFinished."ITEM_COUNT_COMPLETED"]]>
	</queryString>
	<field name="JobFinished_ITEM_COUNT_SUM" class="java.lang.Integer"/>
	<field name="JobFinished_ITEM_COUNT_COMPLETED_SUM" class="java.lang.Integer"/>
	<field name="JobFinished_ITEM_COUNT_COUNT" class="java.lang.Integer"/>
	<field name="JobFinished_ITEM_COUNT_COMPLETED_COUNT" class="java.lang.Integer"/>
	<field name="JobFixed_PRODUCT_LEVEL_ITEM_CODE" class="java.lang.String"/>
