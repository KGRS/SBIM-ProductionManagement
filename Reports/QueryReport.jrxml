	<queryString>
		<![CDATA[SELECT
     ProductLevel2."PL2_ITEM_NAME" AS ProductLevel2_PL2_ITEM_NAME,
     ProductLevel2."PL2_ITEM_PRINT_NAME" AS ProductLevel2_PL2_ITEM_PRINT_NAME,
     ProductLevel2."UnitCode" AS ProductLevel2_UnitCode,
     ProductLevel2."VISIBILITY" AS ProductLevel2_VISIBILITY,
     JobFixed."JOB_FIXED_ID" AS JobFixed_JOB_FIXED_ID,
     JobFixed."JOB_FIXED_NAME" AS JobFixed_JOB_FIXED_NAME,
     JobFixed."PRODUCT_LEVEL" AS JobFixed_PRODUCT_LEVEL,
     JobFixed."PRODUCT_LEVEL_ITEM_CODE" AS JobFixed_PRODUCT_LEVEL_ITEM_CODE,
     JobFixed."ITEM_COUNT" AS JobFixed_ITEM_COUNT,
     JobFixed."ALLOCATED_TIME" AS JobFixed_ALLOCATED_TIME,
     JobFixed."EMPLOYEE_COUNT" AS JobFixed_EMPLOYEE_COUNT,
     JobFixed."REMARKS" AS JobFixed_REMARKS,
     JobFixed."WORK_FLOW_CODE" AS JobFixed_WORK_FLOW_CODE,
     Workflow."WORK_FLOW_NAME" AS Workflow_WORK_FLOW_NAME
FROM
     "dbo"."ProductLevel2" ProductLevel2 INNER JOIN "dbo"."JobFixed" JobFixed ON ProductLevel2."PL2_ITEM_CODE" = JobFixed."PRODUCT_LEVEL_ITEM_CODE"
     INNER JOIN "dbo"."Workflow" Workflow ON JobFixed."WORK_FLOW_CODE" = Workflow."WORK_FLOW_CODE"
WHERE
     JobFixed."JOB_FIXED_ID" = 'FJOB1002']]>
	</queryString>
